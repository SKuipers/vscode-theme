	"tokenColors": [
		{
			"settings": {
				"foreground": "#1c1d20ff",
				"background": "#e2dfd8ff"
			}
		},
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#d45000"
			}
		},
		{
			"name": "Doc Comments",
			"scope": [
				"comment.block",
				"comment.block.documentation",
				"punctuation.definition.comment.ts"
			],
			"settings": {
				"foreground": "#727069"
			}
		},
		{
			"name": "Storage Types",
			"scope": "storage.type",
			"settings": {
				"foreground": "#B052A1"
			}
		},
		{
			"name": "KeywordOperators",
			"scope": "keyword.operator.new, keyword.operator.expression.of, keyword.operator.expression.typeof",
			"settings": {
				"foreground": "#B052A1"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"foreground": "#B052A1"
			}
		},
		{
			"name": "Control Keywords",
			"scope": "keyword.control",
			"settings": {
				"foreground": "#1A1A1A"
			}
		},
		{
			"name": "Operators",
			"scope": "keyword.operator, operator.annotation.type.keyword, var.expr.primitive.annotation.type.keyword.meta",
			"settings": {
				"foreground": "#1A1A1A"
			}
		},
		{
			"name": "Logical Operators",
			"scope": "logical.operator.meta",
			"settings": {
				"foreground": "#1A1A1A"
			}
		},
		{
			"name": "Braces",
			"scope": "punctuation.definition.block, punctuation.terminator.statement, punctuation.definition.parameters, punctuation.separator.comma, meta.property-list.scss, meta.namespace.declaration.ts, meta.brace, bracket.php, meta.function-call.php, punctuation.section.array.end.php, punctuation.section.array.begin.php, punctuation.definition.arguments.begin.bracket.round.php, punctuation.definition.arguments.end.bracket.round.php",
			"settings": {
				"foreground": "#1A1A1A"
			}
		},
		{
			"name": "Accessors",
			"scope": "punctuation.accessor",
			"settings": {
				"foreground": "#1A1A1A"
			}
		},
		
		
		{
			"name": "RegExp",
			"scope": "string",
			"settings": {
				"foreground": "#3778B7"
			}
		},
		{
			"name": "String Interpolation Begin and End",
			"scope": "begin.expression.string, end.expression.string",
			"settings": {
				"foreground": "#B052A1"
			}
		},
		{
			"name": "String Interpolation Body",
			"scope": "expression.string",
			"settings": {
				"foreground": "#1A1A1A"
			}
		},
		{
			"name": "Number",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#3778B7"
			}
		},
